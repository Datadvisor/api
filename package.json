    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "cuid": "^2.1.8",
    "ejs": "^3.1.8",
    "env-var": "^7.3.0",
    "express-session": "^1.17.3",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nest-winston": "^1.8.0",
    "nestjs-session": "^2.0.0",
    "redis": "^4.4.0",
    "reflect-metadata": "^0.1.13",
