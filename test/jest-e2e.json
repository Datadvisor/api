{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "@app/config": "<rootDir>../src/config",
    "@app/logger": "<rootDir>../src/logger"
  },
  "rootDir": "./",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
