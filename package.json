{
  "name": "api",
  "version": "1.0.0",
  "description": "API of Datadvisor",
  "author": "Datadvisor",
  "repository": "https://github.com/Datadvisor/api",
  "license": "MIT",
  "private": true,
  "main": "src/main.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn db:generate && nest build",
    "start": "yarn db:generate && yarn db:migrate && nest start",
    "start:dev": "yarn db:generate && yarn db:migrate && nest start --watch",
    "start:debug": "yarn db:generate && yarn db:migrate && nest start --debug --watch",
    "start:prod": "yarn db:generate && yarn db:migrate:prod && node dist/main",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:format": "prisma format",
    "db:reset": "prisma migrate reset",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.json --verbose --passWithNoTests",
    "test:watch": "jest --config ./jest.json --watch --verbose --passWithNoTests",
    "test:cov": "jest --config ./jest.json --coverage --verbose --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./jest.json --runInBand --verbose --passWithNoTests",
    "test:e2e": "yarn db:generate && yarn db:migrate && jest --config ./test/jest-e2e.json --runInBand --verbose --passWithNoTests",
    "test:e2e:watch": "yarn db:generate && yarn db:migrate && jest --config ./test/jest-e2e.json --watch --runInBand --verbose --passWithNoTests",
    "test:e2e:cov": "yarn db:generate && yarn db:migrate && jest --config ./test/jest-e2e.json --coverage --runInBand --verbose --passWithNoTests",
    "test:e2e:debug": "yarn db:generate && yarn db:migrate && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-e2e.json --runInBand --verbose --passWithNoTests"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
