{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "testSequencer": "./test/utils/jest-sequencer.js",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "src/auth/**/*.(t|j)s",
    "src/contact/**/*.(t|j)s",
    "src/email-confirmation/**/*.(t|j)s",
    "src/reset-password/**/*.(t|j)s",
    "src/users/**/*.(t|j)s",
    "!src/**/*.spec.(t|j)s"
  ],
  "coverageDirectory": "./coverage-e2e"
}
